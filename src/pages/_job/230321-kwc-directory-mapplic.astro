    <script is:inline>
      $(document).ready(function () {
        var css =
          ".mapplic-filtered svg [id^=landmark] > * {opacity: 1 !important; }";

        var mapplic = $("#mapplic").mapplic({
          source: "/vendor-mapplic/mall.json",
          customcss: css,
          sidebar: true,
          height: 500,
          search: true,
          searchdescription: true,
          minimap: false,
          marker: "hidden",
          fillcolor: false,
          fullscreen: true,
          developer: false,
          thumbholder: true,
          maxscale: 3,
        });
      });
